					"script": {
						"exec": [
							"// Obtiene el cuerpo de la respuesta como un objeto JSON\r",
							"const responseJson = pm.response.json();\r",
							"// Extrae el valor del token del objeto JSON y guárdalo en la variable 'token'\r",
							"var token = responseJson.token;\r",
							"// Guarda la variable en el entorno de Postman con el nombre 'bearerToken'\r",
							"pm.environment.set(\"bearerToken\", token);"
